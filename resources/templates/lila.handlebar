<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Nitya Lila Reminder</title>

    <!-- Bootstrap Core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <link href="css/font-awesome.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="css/one-page-wonder.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>
    <!-- ************* INDEX **************** -->
    <div id="index" style="display:none;">{{index}}</div>
    <!-- ************************************ -->

    <!-- Navigation -->
    <header class="navbar navbar-inverse navbar-static-top">
      <div class="container">
        <ul id="current-time" class="nav navbar-nav navbar-left">
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <li>
            <a id="github-link" href="https://github.com/nimaai/nlr-web" target="_blank">
              <i class="fa fa-github"></i>
            </a>
          </li>
          <li>
            <a id="fb-link" href="https://facebook.com/nlreminder" target="_blank">
              <i class="fa fa-facebook-square"></i>
            </a>
          </li>
        </ul>
      </div>
    </header>

    <!-- Full Width Image Header -->
    <header class="header-image">
      <div class="headline">
        <div class="container">
          <h1 id="sanskrit-name">{{sanskritName}}</h1>
          <h2 id="english-name">{{englishName}}</h2>
          <h2 id="time-interval">{{timeInterval}}</h2>
        </div>
      </div>
    </header>

    <div id="short-description" class="hidden">{{shortDescription}}</div>

    <!-- Page Content -->
    <div class="container">

      <hr class="featurette-divider">

      <!-- First Featurette -->
      <div class="featurette" id="rupa">
        <img class="featurette-image img-circle img-responsive pull-right" src="images/rupa-goswami.jpg">
        <h2 class="featurette-heading">Rupa Goswami</h2>
        <p id="rupa-description" class="lead">{{rupaDescription}}</p>
      </div>

      <hr class="featurette-divider">

      <!-- Second Featurette -->
      <div class="featurette" id="vishvanath">
        <img class="featurette-image img-circle img-responsive pull-left" src="images/vishvanath-chakravarti.jpg">
        <h2 class="featurette-heading">Vishvanath Chakravarti</h2>
        <p id="vishvanath-description" class="lead">{{vishvanathDescription}}</p>
      </div>

      <hr class="featurette-divider">

      <!-- Third Featurette -->
      <div class="featurette" id="long">
        <img class="featurette-image img-circle img-responsive pull-right" src="images/pancha-tattva.jpg">
        <h2 class="featurette-heading">Dive deeper...</h2>
        <p id="long-description" class="lead">{{longDescription}}</p>
      </div>

      <hr class="featurette-divider">

      <!-- Footer -->
      <footer>
        <div class="row">
          <div class="col-lg-6">
            <p>Copyright &copy; Supreme Lords Nityananda Gauranga 2015</p>
          </div>
        </div>
      </footer>
    </div>

    <script type="text/javascript" src="js/dist.js"></script>
  </body>
</html>
